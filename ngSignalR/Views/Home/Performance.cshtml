@{
    Layout = null;
}
<h1 class="page-header">Performance</h1>
<div class="row">
    <div class="col-lg-12">
        <h3>Categories</h3>
        <table class="table table-striped table-bordered table-hover">
            <tbody>
                <tr ng-repeat="category in model.categories">
                    <td style="cursor: pointer">
                        <div class="clearfix">
                            <h4 style="margin: 0;display: inline-block">{{category.categoryName}}</h4>
                            <small class="label label-primary pull-right">{{category.machineName === '.' ? 'localhost' : category.machineName}}</small>&nbsp;
                            <small class="label label-warning pull-right">{{category.categoryType}}</small>
                        </div>
                        <small>{{category.categoryHelp}}</small>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>


